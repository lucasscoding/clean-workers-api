    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.12",
